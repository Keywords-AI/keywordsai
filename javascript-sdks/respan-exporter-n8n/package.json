{
	"name": "@respan/n8n-nodes-respan",
	"version": "0.1.6",
	"description": "Respan Exporter for n8n",
	"license": "MIT",
	"homepage": "https://respan.ai",
	"keywords": [
		"n8n-community-node-package",
		"respan",
		"llm",
		"ai"
	],
	"author": {
		"name": "Respan",
		"email": "team@respan.ai"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/respan-ai/respan-sdks",
		"directory": "javascript-sdks/respan-exporter-n8n"
