{
  "name": "@respan/respan-sdk",
  "type": "module",
  "version": "0.0.1",
  "description": "Respan typing SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Respan <team@respan.ai>",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "version": "npm version",
    "publish": "yarn npm publish",
    "release": "yarn build &&yarn version patch && yarn publish",
    "example:simple-evaluator": "npx tsx examples/simple-evaluator-example.ts",
    "example:logs-operation": "npx tsx examples/logs-operation-example.ts",
    "example:sdk-structure": "npx tsx examples/sdk-structure-demo.ts",
    "example:dataset-workflow": "npx tsx examples/dataset-workflow-example.ts",
    "example:experiment-workflow": "npx tsx examples/experiment-workflow-example.ts",
    "example:flexible-input": "npx tsx examples/flexible-input-example.ts",
    "examples": "npm run example:sdk-structure && npm run example:simple-evaluator && npm run example:logs-operation"
  },
  "packageManager": "yarn@4.6.0",
  "dependencies": {
    "@respan/respan-sdk": "^0.0.15"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "dotenv": "^17.2.1",
    "tsx": "^4.20.3"
  }
}
